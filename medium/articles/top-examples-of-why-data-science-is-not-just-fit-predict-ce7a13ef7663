 guide"},"collection":{"__ref":"Collection:98111c9905da"},"title":"Deploy a Python Machine Learning Model on your iPhone","mediumUrl":"https:\u002F\u002Fpub.towardsai.net\u002Fdeploy-a-python-machine-learning-model-on-your-iphone-d006b1e04bc4","creator":{"__ref":"User:bd0bff93b38f"},"previewImage":{"__ref":"ImageMetadata:1*He_DVTn2N7_-p2ZPVmpNwA.jpeg"},"clapCount":231,"isSeries":false,"sequence":null,"uniqueSlug":"deploy-a-python-machine-learning-model-on-your-iphone-d006b1e04bc4"},"User:e35df1e833d5":{"__typename":"User","id":"e35df1e833d5","imageId":"0*r_pBqbjK8j1-aBzF.jpg","mediumMemberAt":0,"name":"FMZ Quant","username":"FMZ_Quant","customDomainState":null,"hasSubdomain":false,"bio":"💻Building an extraordinary quantitative world #quants #traders #quantitative #strategy #Bitcoin #cryptocurrency #Futures #Stocks https:\u002F\u002Ft.me\u002FFMZQuantq"},"ImageMetadata:0*9c0ovFlKWtVYvPmQ.png":{"__typename":"ImageMetadata","id":"0*9c0ovFlKWtVYvPmQ.png","alt":null,"focusPercentX":null,"focusPercentY":null},"Post:7f1dc6f72971":{"__typename":"Post","id":"7f1dc6f72971","visibility":"PUBLIC","previewContent":{"__typename":"PreviewContent","isFullContent":false,"subtitle":"Summary"},"collection":null,"title":"Analysis and Realization of Commodity Futures Volume Footprint Chart","mediumUrl":"https:\u002F\u002Fmedium.com\u002F@FMZ_Quant\u002Fanalysis-and-realization-of-commodity-futures-volume-footprint-chart-7f1dc6f72971","creator":{"__ref":"User:e35df1e833d5"},"previewImage":{"__ref":"ImageMetadata:0*9c0ovFlKWtVYvPmQ.png"},"clapCount":0,"isSeries":false,"sequence":null,"uniqueSlug":"analysis-and-realization-of-commodity-futures-volume-footprint-chart-7f1dc6f72971"},"User:b64c852d31de":{"__typename":"User","id":"b64c852d31de","imageId":"0*49EXkeL_4Mf5s_jI","mediumMemberAt":0,"name":"Bhagyashripagar","username":"bhagyashripagar55","customDomainState":null,"hasSubdomain":false,"bio":""},"Post:3dc7b87c2ba1":{"__typename":"Post","id":"3dc7b87c2ba1","visibility":"PUBLIC","preview